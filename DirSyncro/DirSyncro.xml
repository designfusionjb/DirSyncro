<?xml version="1.0" encoding="UTF-8"?>
<DirSyncro xsi:schemaLocation="DirSyncroConfiguration DirSyncro.xsd" xmlns="DirSyncroConfiguration" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Watcher Name="Watcher 1" Enabled="true">
    <SourceDirectory>C:\Temp</SourceDirectory>
    <TargetDirectory>C:\Target1</TargetDirectory>
    <Recursive>true</Recursive>
    <Versions>3</Versions>
    <Settling>10000</Settling>
    <Retention>100</Retention>
    <Include></Include>
    <Exclude>*.tmp; ~*.docx</Exclude>
  </Watcher>
</DirSyncro>